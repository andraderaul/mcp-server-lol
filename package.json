{
  "name": "mcp-server-lol",
  "version": "1.0.0",
  "description": "MCP Server project with TypeScript and Biome",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "lint": "biome lint src/",
    "lint:fix": "biome lint --write src/",
    "format": "biome format src/",
    "format:fix": "biome format --write src/",
    "check": "biome check src/",
    "check:fix": "biome check --write src/"
  },
  "keywords": [
    "mcp",
    "typescript",
    "server"
  ],
  "author": "https://github.com/andraderaul/mcp-server-lol",
  "license": "GPL-3.0",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.0",
    "dotenv": "^16.5.0",
    "zod": "^3.25.32",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.15.29",
    "typescript": "^5.7.2"
  }
}
